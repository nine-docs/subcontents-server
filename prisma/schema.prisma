generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Bookmark {
  id         BigInt    @id @default(autoincrement())
  user_id    BigInt
  article_id BigInt
  created_at DateTime? @default(now()) @db.Timestamp(0)

  @@map("bookmark")
}
